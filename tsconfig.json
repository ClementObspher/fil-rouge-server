{
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ES2020", "DOM"],
		"skipLibCheck": true,
		"types": ["bun-types"]
	}
}
